<configuration>
    <!-- 流数据接入 -->
    <DataSource>
        <stream_id>1</stream_id>
        <stream_name>mq2kafka_stream</stream_name>
        <stream_type>kafka</stream_type>
        <zkQuorum></zkQuorum>
        <topics>com.ailk.flowmanagement.mcd.flux_action</topics>
        <groupId>group</groupId>
        <consumerNum>2</consumerNum>
        <stream_columns>business_id,user_account,business_start_time,content_type,keyword,site_id,activity_code</stream_columns>
    </DataSource>

    <step>
        <Input>imsi,city_id</Input>  dstream
        <HBaseTable>htable</HBaseTable> hbase
        <HBaseKey>DStreamName(imsi)</HBaseKey>
        <HBaseValue>city_id:CITY_ID</HBaseValue>
        <where>city_id != city_id</where>
        <output>imsi,city_id</output>

    </step>
</configuration>